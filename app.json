{
  "expo": {
    "name": "Anifuri",
    "slug": "anifuri",
    "version": "0.7.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "anifuri",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#201f31"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#201f31"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.maxxdevs.anifuri"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "transparent",
      "translucent": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true,
          "startsPictureInPictureAutomatically": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "566367b3-a7e2-44e9-a6d7-b4c90e17e2f1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/566367b3-a7e2-44e9-a6d7-b4c90e17e2f1"
    }
  }
}
